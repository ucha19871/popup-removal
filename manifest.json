{
  "manifest_version": 2,
  "name": "PoPup Removal",
  "version": "0.2",
  "description": "Application to remove PoPups on various sites",
  "icons": {"16":"icon.png"},
  "author": "Ucha",
  "background": "background.html",
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": ["jquery.js","main.js"]
  } ],
  "browser_action": {
          "default_icon": {"19": "icon.png"},
          "default_title": "PoPup Removal",
          "default_popup": "popup.html"
        },
  "permissions": ["tabs"]
}
